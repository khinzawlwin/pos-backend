
      <!-- Content Header (Page header) -->
      <section class="content-header">
        <h1>
         Edit Townships
        </h1>
       
      </section>
  
      <!-- Main content -->
      <section class="content">
        <div class="box box-solid box-info">
            <div class="box-body">
                    <form action="/townships/<%= township.id %>/update" method="POST" >

                        <div class="form-group">
                            <label for="">Township (EN)</label>
                            <input type="text" class="form-control" name="township_en" value="<%= township.township_en %>"/>
                       </div>
                       <div class="form-group">
                            <label for="">Township (MM)</label>
                            <input type="text" class="form-control" name="township_mm" value="<%= township.township_mm %>"/>
                       </div>
                       <div class="form-group">
                            <label for="">City Id</label>
                            <input type="text" class="form-control" name="city_id" value="<%= township.city_id %>"/>
                       </div>
                    
                        <div class="form-group">
                            <button class="btn btn-success">Submit</button>
                            <a href="/townships" class="btn btn-warning">Cancel</a>
                        </div>
                    
                    </form>
            </div>
        </div>   
        
   </section>
</section>